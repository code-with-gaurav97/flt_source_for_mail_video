<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="content-type" content="text/html" charset="utf-8">
</head>
<body>
<style>
    body{
        padding: 0 !important;
        margin: 0 !important;
    }
    .table-style{
        height: 100% !important;
        width: 100% !important;
        background-color: #f1f1f1;
        min-width: 600px;
    }
    .td-style{
        align-content: center;
        vertical-align: top;
        width: 100%;
        min-width: 600px;
    }
    .center-table-style{
        width: 100%;
        min-width: 500px;
        border: 0;
        background-color: #f1f1f1;
    }
    .center-table-style2{
        width: 100%;
        min-width: 500px;
        border: 0;
    }
    .tr-td-style{
        width: 100%;
        line-height:1px;
        font-size:1px;
    }
    .minw500{
        min-width: 500px;
    }
    .maxh50{
        max-height:40px
    }
    .img-style{
        max-width:250px;height:auto;display:block;margin:0px;
    }
    .tr-td-style1{
        font-family:arial,helvetica,sans-serif;
        font-size:26px;
        color:#313131;
        line-height:32px;
    }
    .tr-td-style2{
        width: 100%;
        line-height:1px;font-size:1px;
    }
    .confirmation{
        font-family:sans-serif;
        color:#202020;
        text-align:center;
        font-size:26px;
        line-height:32px;
        line-height:100%;
        letter-spacing:2px;
    }
    .code-confirmation-td{
        width: 100%;
        line-height:1px;font-size:1px;
    }
    .table2{
        width: 500px;
        background-color:#fff;
        margin:0 auto;
    }
    .td-style1{
        line-height:1px;font-size:1px;
    }
    .hello-user{
        font-family:arial,helvetica,sans-serif;
        font-size:14px;
        color:#202020;
        line-height:19px;
        line-height:134%;
        letter-spacing:.5px;
    }
    .minw400{
        min-width:400px;
        background-color:#fff;
    }
    .below-minw400{
        font-family:arial,helvetica,sans-serif;
        font-size:14px;color:#202020;line-height:19px;line-height:134%;
    }
    .need-help{
        font-family:arial,helvetica,sans-serif;font-weight:bold;
        font-size:10px;color:#202020;text-align:center;line-height:12px;
    }
    .address{
        width: 100%;
        font-family:arial,helvetica,sans-serif;font-size:10px;
        color:#858585;text-align:center;line-height:12px;
    }
    .atcodewithgaurav{
        text-decoration:none;color:#037aee;
    }
    .rights-reserved{
        font-family:arial,helvetica,sans-serif;
        font-size:10px;
        color:#202020;
        text-align:center;
        line-height:12px;
    }
</style>
<table class="table-style">
    <tbody>
    <tr>
        <td class="td-style">
            <center>
                <table cellpadding="0" cellspacing="0" class="center-table-style">
                    <tbody>
                    <tr>
                        <td align="center">
                            <table class="center-table-style2" cellpadding="0"
                                   cellspacing="0">
                                <tbody>
                                <tr height="50">
                                    <td height="50" class="tr-td-style">
                                        &nbsp;</td>
                                </tr>
                                <tr>
                                    <td align="center">
                                        <table border="0" cellpadding="0" cellspacing="0"
                                               class="minw500">
                                            <tbody>
                                            <tr>
                                                <td valign="middle" align="center">
                                                    <div class="maxh50">
                                                        <div>
                                                            <a href="${redirect_url}"
                                                               target="_blank">
                                                                <img
                                                                        align="none" alt="Code with Gaurav"
                                                                        border="0" hspace="0"
                                                                        src="${code_with_gaurav_image}"
                                                                        class="img-style"
                                                                        title="Code with Gaurav" vspace="0"
                                                                        width="55px" class="CToWUd"
                                                                        data-bit="iit">
                                                            </a>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                            <table>
                                <tbody>
                                <tr>
                                    <td align="center"
                                        class="tr-td-style1">
                                        &nbsp;</td>
                                </tr>
                                <tr>
                                    <td align="center">&nbsp;</td>
                                </tr>
                                </tbody>
                            </table>
                            <table class="center-table-style2" cellpadding="0"
                                   cellspacing="0">
                                <tbody>
                                <tr>
                                    <td align="center">
                                        <table cellpadding="0" cellspacing="0" class="center-table-style2">
                                            <tbody>
                                            <tr>
                                                <td height="50"
                                                    class="tr-td-style2">
                                                    <div
                                                            class="confirmation">
                                                        Thanks for choosing us!!
                                                    </div>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td height="50"
                                                    class="code-confirmation-td">
                                                    <div
                                                            class="confirmation">
                                                        <a href="${confirmation_url_key}"
                                                           target="_blank">${confirmation_message}</a>
                                                    </div>
                                                </td>
                                            </tr>
                                            </tbody>
                                        </table>
                                        <table class="center-table-style2"
                                               cellpadding="0" cellspacing="0">
                                            <tbody>
                                            <tr height="40">
                                                <td align="center">&nbsp;</td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                    </tbody>
                </table>
                <table width="500" border="0" cellpadding="0" cellspacing="0"
                       class="table2">
                    <tbody>
                    <tr height="50">
                        <td width="50">&nbsp;</td>
                        <td height="50" class="td-style1">&nbsp;</td>
                        <td width="50">&nbsp;</td>
                    </tr>
                    <tr>
                        <td width="50">&nbsp;</td>
                        <td align="left"
                            class="hello-user">
                            Hello ${user_name},
                            <br><br>
                        </td>
                        <td width="50">&nbsp;</td>
                    </tr>
                    <tr>
                        <td width="50">&nbsp;</td>
                        <td align="left"
                            class="hello-user">
                            You recently tried to create an account with us. In order to
                            complete your Signup process, please click the confirmation url.
                            <a style="text-decoration:none"
                               href="${redirect_url}"
                               target="_blank">Learn
                                more</a>.
                            <br><br>
                        </td>
                        <td width="50">&nbsp;</td>
                    </tr>
                    </tbody>
                </table>
                <table width="500" border="0" cellpadding="0" cellspacing="0"
                       class="minw400">
                    <tbody>
                    <tr>
                        <td width="50">&nbsp;</td>
                        <td align="left"
                            class="below-minw400">
                            <div
                                    class="hello-user">
                                <br>
                                Thank you,<br>Code with Gaurav
                            </div>
                        </td>
                        <td width="50">&nbsp;</td>
                    </tr>
                    <tr height="50">
                        <td width="50">&nbsp;</td>
                        <td height="50" style="line-height:1px;font-size:1px">&nbsp;</td>
                        <td width="50">&nbsp;</td>
                    </tr>
                    </tbody>
                </table>
                <table class="center-table-style2" cellpadding="0" cellspacing="0">
                    <tbody>
                    <tr>
                        <td align="center">
                            <table class="center-table-style2" cellpadding="0" cellspacing="0">
                                <tbody>
                                <tr height="50">
                                    <td height="20" class="tr-td-style">
                                        &nbsp;</td>
                                </tr>
                                <tr>
                                    <td align="center">
                                        <div
                                                class="need-help">
                                            Need Help?
                                            <!-- url https://www.youtube.com/@CodewithGaurav97 -->
                                            <a class="atcodewithgaurav"
                                               href="${redirect_url}"
                                               target="_blank">
                                                @CodewithGaurav97</a><br>
                                        </div>
                                    </td>
                                </tr>
                                <tr height="30">
                                    <td class="code-confirmation-td" height="20" >
                                        &nbsp;</td>
                                </tr>
                                <tr>
                                    <td align="center">
                                        <div
                                                class="rights-reserved">
                                            <p>
                                                © 2023, Code with Gaurav. All rights reserved.
                                            </p>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                </tr>
                                <tr height="20">
                                    <td height="20" align="center"
                                        class="address">
                                        Code with Gaurav, India.</td>
                                </tr>
                                <tr>
                                </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                    </tbody>
                </table>
            </center>
        </td>
    </tr>
    </tbody>
</table>
</body>
</html>



